    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "GIT_COMMIT_HASH=$(git rev-parse HEAD) node scripts/build.js",
    "heroku-postbuild": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "precommit": "lint-staged && flow check",
    "seed": "node seeder/index.js",
    "svgr": "npx @svgr/cli -d ./src/icons/ ./src/resources/ --template ./svgr-template.js --ext jsx --no-dimensions --replace-attr-values=\"#2E2E2E=currentColor\""
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^8.0.4",
    "empty": "^0.10.1",
    "firebase-admin": "^5.12.1",
    "firebase-tools": "^4.1.2",
    "firestore-seed": "0.0.5",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.5.1",
    "generate-robotstxt": "^5.0.7",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
